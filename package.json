{
  "name": "lush",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dev:remote": "concurrently -n \"lush-db,lush-server\" -c \"cyan,blue\" \"npm run db:docker:start\" \"cross-env DATABASE_URL=mysql://root:lush@127.0.0.1:3307/lush next dev\"",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "build-start": "npm run build && next start",
    "start:init": "npm ci && npm run build-start:remote",
    "start:remote": "concurrently -n \"lush-db,lush-server\" -c \"cyan,green\" \"npm run db:docker:start\" \"cross-env DATABASE_URL=mysql://root:lush@127.0.0.1:3307/lush next start\"",
    "build-start:remote": "npm run build && npm run start:remote",
    "db:dump": "cd ./database && dump-lush-db.bat",
    "db:docker:build": "docker build -t demidv/lush-db:latest ./database",
    "db:docker:start": "docker start -ai lush-db || npm run db:docker:run",
    "db:docker:run": "docker run --name lush-db -p 3307:3307 -e MYSQL_TCP_PORT=3307 -e MYSQL_ROOT_PASSWORD=lush demidv/lush-db:latest"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@tanstack/react-query": "^4.16.0",
    "@trpc/client": "^10.0.0",
    "@trpc/next": "^10.0.0",
    "@trpc/react-query": "^10.0.0",
    "@trpc/server": "^10.0.0",
    "next": "13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.9.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.0.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.7.1",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.10.2"
  }
}
